{
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "quotes": ["error", "double", { "allowTemplateLiterals": true }],
      "semi": [2, "never"],
      "space-before-function-paren": [2, "always"],
      "no-console": [1]
    }
}
